{
  "name": "desktop-linux",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "packages/desktop/linux",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm run build",
        "cwd": "packages/desktop/linux"
      },
      "dependsOn": [
        {
          "projects": ["shared"],
          "target": "build"
        }
      ]
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm run dev",
        "cwd": "packages/desktop/linux"
      }
    },
    "package": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm run package",
        "cwd": "packages/desktop/linux"
      },
      "dependsOn": [
        {
          "target": "build",
          "projects": "self"
        }
      ]
    }
  },
  "tags": ["app", "desktop", "linux"],
  "implicitDependencies": ["desktop"]
}
{
  "name": "web-common",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "packages/web/common",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm run build",
        "cwd": "packages/web/common"
      },
      "dependsOn": [
        {
          "projects": ["shared"],
          "target": "build"
        }
      ]
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm run test",
        "cwd": "packages/web/common"
      }
    }
  },
  "tags": ["lib", "web", "common"],
  "implicitDependencies": []
}
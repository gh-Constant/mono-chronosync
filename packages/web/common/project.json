{
  "name": "web-common",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/web/common/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/web/common",
        "main": "packages/web/common/src/index.ts",
        "tsConfig": "packages/web/common/tsconfig.json",
        "assets": ["packages/web/common/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    }
  },
  "tags": ["scope:web", "type:common"]
}
{
  "name": "web-frontend",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/web/frontend/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/web/frontend",
        "command": "vite build"
      },
      "dependsOn": [
        {
          "projects": ["web-common"],
          "target": "build"
        }
      ]
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/web/frontend",
        "command": "vite"
      }
    },
    "serve-prod": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/web/frontend",
        "command": "vite preview --host 0.0.0.0 --port $PORT --base /"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    }
  },
  "tags": ["scope:web", "type:frontend"]
}
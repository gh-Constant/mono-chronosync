{
  "name": "web-backend",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/web/backend/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/web/backend",
        "main": "packages/web/backend/src/index.ts",
        "tsConfig": "packages/web/backend/tsconfig.json",
        "assets": [
          "packages/web/backend/*.md",
          {
            "glob": "**/*.sql",
            "input": "packages/web/backend/src",
            "output": "src"
          }
        ]
      },
      "dependsOn": [
        {
          "projects": ["web-common"],
          "target": "build"
        }
      ]
    },
    "serve": {
      "executor": "@nx/js:node",
      "options": {
        "buildTarget": "web-backend:build"
      }
    },
    "start": {
      "executor": "@nx/js:node",
      "options": {
        "buildTarget": "web-backend:build"
      },
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    }
  },
  "tags": ["scope:web", "type:backend"]
}
{
  "name": "chronosync",
  "version": "1.0.0",
  "description": "ChronoSync monorepo",
  "private": true,
  "scripts": {
    "setup": "pnpm install",
    "dev": "nx run-many --target=dev --projects=web-frontend,web-backend,web --parallel",
    "dev:web": "nx run-many --target=dev --projects=web-frontend,web-backend --parallel",
    "build": "nx run-many --target=build --all",
    "start": "nx run web:start",
    "test": "nx run-many --target=test --all",
    "dev:desktop": "nx run desktop:dev",
    "dev:desktop:windows": "nx run desktop-windows:dev",
    "dev:desktop:mac": "nx run desktop-mac:dev",
    "dev:desktop:linux": "nx run desktop-linux:dev",
    "dev:mobile": "nx run mobile:dev",
    "dev:mobile:android": "nx run mobile-android:dev",
    "dev:mobile:ios": "nx run mobile-ios:dev",
    "dev:web:frontend": "nx run web-frontend:dev",
    "dev:web:backend": "nx run web-backend:dev",
    "package": "nx run-many --target=package --all",
    "package:desktop": "nx run desktop:package",
    "package:mobile": "nx run mobile:package",
    "graph": "nx graph",
    "dev:all": "nx run-many --target=dev --all --parallel",
    "start:web": "nx run-many --target=start --projects=web-frontend,web-backend --parallel",
    "start:web:frontend": "nx run web-frontend:preview",
    "start:web:backend": "nx run web-backend:start",
    "build:web:frontend": "nx run web-frontend:build",
    "build:web:backend": "nx run web-backend:build",
    "init:db": "nx run web-backend:init-db",
    "build:web:common": "nx run web-common:build",
    "build:web": "nx run-many --target=build --projects=web-frontend,web-backend,web-common --parallel",
    "serve:web:frontend": "serve packages/web/frontend/dist -s -l 3000"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@nx/eslint": "20.5.0",
    "@nx/vite": "20.5.0",
    "@nx/web": "20.5.0",
    "@nx/workspace": "^20.5.0",
    "@vitest/ui": "^3.0.0",
    "eslint": "~8.57.0",
    "jiti": "2.4.2",
    "nx": "20.5.0",
    "typescript": "^5.0.0",
    "vite": "^6.0.0",
    "vitest": "^3.0.0",
    "serve": "^14.2.1"
  },
  "packageManager": "pnpm@10.6.1",
  "nx": {},
  "dependencies": {}
}
